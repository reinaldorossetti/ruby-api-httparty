{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "nome": "Keenan Waters",
            "email": "aron@williamson-osinski.io",
            "password": "LgSvJyNeEjYi90A",
            "administrador": "true"
        }
    ],
    "required": [
        "nome",
        "email",
        "password",
        "administrador"
    ],
    "properties": {
        "nome": {
            "$id": "#/properties/nome",
            "type": "string",
            "title": "The nome schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Keenan Waters"
            ]
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The email schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "aron@williamson-osinski.io"
            ]
        },
        "password": {
            "$id": "#/properties/password",
            "type": "string",
            "title": "The password schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "LgSvJyNeEjYi90A"
            ]
        },
        "administrador": {
            "$id": "#/properties/administrador",
            "type": "string",
            "title": "The administrador schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "true"
            ]
        }
    },
    "additionalProperties": true
}